{
  "profiles": {
    "MyApplication.hellohelm": {
      "commandName": "Project",
      "launchBrower": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:7218"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": false,
      "httpPort": 8080,
      "useSSL": false,
      "environmentVariables": {
        "ASPNETCORE_URL": "http://+.8080",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}