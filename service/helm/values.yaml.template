tokens:
%%[START]%%
  - name: '%[NAME]%'
    value: '%[VALUE]%'
%%[END]%%

swag:
  image:
    name: "%[swag_image]%"
    tag: "%[swag_image_tag]%"
    pullPolicy: "%[swag_image_pullPolicy]%"
  replicas: %[swag_replicas]%
  httpScheme: "%[swag_httpScheme]%"
  tlssecret: "%[swag_tlssecret]%"
  bam:
    enabled: %[swag_bam_enabled]%
  job:
    enabled: %[swag_job_enabled]%
    
springrestapi:
  image:
    name: "%[spring-rest-api_image]%"
    tag: "%[spring-rest-api_image_tag]%"
    pullPolicy: "%[spring-rest-api_image_pullPolicy]%"
  replicas: %[spring-rest-api_replicas]%
  httpScheme: "%[spring-rest-api_httpScheme]%"
  
loadbalance:
  name: "%[swag_lbl_name]%"
  host: "%[swag_lbl_host]%"